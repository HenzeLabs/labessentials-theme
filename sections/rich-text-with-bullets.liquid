<section id="rich-text-with-bullets" class="rich-text-section">
  <div class="rich-text-content">
    <h2>{{ section.settings.heading }}</h2>
    <ul>
      {% for bullet in section.settings.bullets %}
        <li>{{ bullet }}</li>
      {% endfor %}
    </ul>
  </div>
</section>

{% schema %}
{
  "name": "Rich Text with Bullets",
  "settings": [
    { "type": "text", "id": "heading", "label": "Heading" },
    { "type": "textarea", "id": "bullets", "label": "Bullets", "default": "Enter bullet points here." }
  ]
}
{% endschema %}
